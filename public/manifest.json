{
  "manifest_version": 3,
  "name": "Leve - Atendimento Clínico",
  "version": "1.0",
  "description": "Assistente de atendimento clínico com IA",
  "permissions": ["activeTab", "microphone"],
  "content_scripts": [
    {
      "matches": ["*://www.google.com.br/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": ["logo.png"]
}